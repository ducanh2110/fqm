<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>io.github.fuquanming</groupId>
		<artifactId>fqm-dynamic-module</artifactId>
		<version>1.0.1</version>
	</parent>
	<artifactId>fqm-dynamic-module-spring-mybatis</artifactId>
	<version>1.0.3</version>
	
    <description>Dynamically load jar based on spring mybatis</description>
    
    <properties>
        <mybatis-spring.version>3.0.0</mybatis-spring.version><!-- 对应mybatis 3.5.10,mybatis-spring 2.0.6 -->
        <fqm-dynamic-module-spring.version>1.0.3</fqm-dynamic-module-spring.version>
    </properties>
    
	<dependencies>
		<dependency>
			<groupId>io.github.fuquanming</groupId>
			<artifactId>fqm-dynamic-module-core</artifactId>
			<version>${parent.version}</version>
		</dependency>
		
		<dependency>
            <groupId>io.github.fuquanming</groupId>
            <artifactId>fqm-dynamic-module-spring</artifactId>
            <version>${fqm-dynamic-module-spring.version}</version>
        </dependency>

		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-web</artifactId>
		</dependency>
		
		<dependency>
            <groupId>io.github.fuquanming</groupId>
            <artifactId>fqm-dynamic-module-mybatis</artifactId>
            <version>${parent.version}</version>
        </dependency>

		<dependency>
			<groupId>org.mybatis</groupId>
			<artifactId>mybatis-spring</artifactId>
			<version>${mybatis-spring.version}</version>
		</dependency>
	</dependencies>
</project>