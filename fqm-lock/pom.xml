            <dependency>
                <groupId>io.github.fuquanming</groupId>
                <artifactId>fqm-common-redis</artifactId>
                <version>${fqm-common-redis.version}</version>
            </dependency>
            <dependency>
                <groupId>io.github.fuquanming</groupId>
                <artifactId>fqm-lock-core</artifactId>
                <version>${fqm-lock-core.version}</version>
            </dependency>
            
            <dependency>
                <groupId>io.github.fuquanming</groupId>
                <artifactId>fqm-common-redisson</artifactId>
                <version>${fqm-common-redisson.version}</version>
            </dependency>
            
            <dependency>
                <groupId>io.github.fuquanming</groupId>
                <artifactId>fqm-common-zookeeper</artifactId>
                <version>${fqm-common-zookeeper.version}</version>
