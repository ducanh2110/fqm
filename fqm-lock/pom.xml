<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>io.github.fuquanming</groupId>
        <artifactId>fqm-dependencies</artifactId>
        <version>1.1.0</version>
        <relativePath>../fqm-dependencies</relativePath>
    </parent>
    
    <properties>
        <fqm-lock.version>1.1.0</fqm-lock.version>
    
        <!-- 内部依赖版本 -->
        <fqm-lock-core.version>${fqm-lock.version}</fqm-lock-core.version>
        <fqm-common-redis.version>${fqm-lock.version}</fqm-common-redis.version>
        <fqm-common-redisson.version>${fqm-lock.version}</fqm-common-redisson.version>
        <fqm-common-zookeeper.version>${fqm-lock.version}</fqm-common-zookeeper.version>
        
